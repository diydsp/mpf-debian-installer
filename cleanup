##!/bin/bash

# This script should be used to cleanup the install directory
# and start again. It should be run as root. It does NOT uninstall
# anything--it only cleans up this folder.
# e.g. sudo ./cleanup

rm -r libftdi1-1.1
rm -r libpinproc
rm -r libusb-1.0.18
rm -r libusbx-1.0.18
rm -r pypinproc
rm -r PyYAML-3.11
rm -r yaml-cpp-0.2.5
